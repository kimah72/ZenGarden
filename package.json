{
  "name": "stylestage-sass",
  "version": "1.1.1",
  "description": "A minimal starter for Style Stage submissions that includes the source Sass used to create the base Style Stage theme, and the source HTML.",
  "main": "index.html",
  "scripts": {
    "scss": "sass --no-source-map sass:public",
    "watch": "sass --no-source-map --watch sass:public",
    "serve": "browser-sync start --server public --files public --port 1337",
    "start": "npm-run-all scss --parallel watch serve"
  },
  "keywords": [
    "style stage",
    "sass"
  ],
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
}
